<Window x:Class="SmokeBall.Desk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SmokeBall.Desk"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Get latest find" HorizontalAlignment="Left" Margin="260,279,0,0" VerticalAlignment="Top" Height="27" Width="107" Click="Button_Click"/>
        <Label Content="Smokeball's top finds" HorizontalAlignment="Left" Margin="238,92,0,0" VerticalAlignment="Top" Height="31" Width="189"/>
        <TextBox x:Name="TextBoxResult" HorizontalAlignment="Left" Margin="209,144,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261" Height="106" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="1.399"/>
                    <RotateTransform/>
                    <TranslateTransform X="0.891"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>

    </Grid>
</Window>
